import _ from "lodash";
import { FETCH_TESTDATA } from "../actions/types";

export default (state = {}, action) => {
  console.log("1.action>>>", action);
  switch (action.type) {
    case FETCH_TESTDATA:
      return { ...state, ..._.mapKeys(action.payload, "id") };
    default:
      return state;
  }
};
